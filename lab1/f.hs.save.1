incAll :: [[Int]] -> [[Int]]
incAll [] = []
incAll (x : xs) = f x : incAll xs where
  f [] = []
  f (y : ys) = (y + 1) : f ys

iA :: [[Int]] -> [[Int]]
iA [] = []
iA (x : xs) = (map (+1) x) : iA xs

iA' :: [[Int]] -> [[Int]]
iA' xs :: map (\ys -> map (+1) ys) xs

iA'' :: [[Int]] -> [[Int]]
iA'' xs :: map (map (+1)) xs  -- częściowa aplikacja funkcji

iA''' :: [[Int]] -> [[Int]]
iA''' = map (map (+1)) xs
